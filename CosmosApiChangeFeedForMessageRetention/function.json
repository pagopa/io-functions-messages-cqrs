{
  "bindings": [
    {
      "type": "cosmosDBTrigger",
      "name": "messageStatusDocuments",
      "direction": "in",
      "leaseCollectionName": "leaseMessageStatusForMessageRetention",
      "leaseCollectionPrefix": "%LEASE_COLLECTION_PREFIX%_",
      "connectionStringSetting": "COSMOSDB_CONNECTION_STRING",
      "databaseName": "%COSMOSDB_NAME%",
      "collectionName": "message-status",
      "createLeaseCollectionIfNotExists": true,
      "startFromBeginning": true
    }
  ],
  "retry": {
    "strategy": "exponentialBackoff",
    "maxRetryCount": 10,
    "minimumInterval": "00:00:05",
    "maximumInterval": "00:01:00"
  },
  "scriptFile": "../dist/CosmosApiChangeFeedForMessageRetention/index.js"
}
