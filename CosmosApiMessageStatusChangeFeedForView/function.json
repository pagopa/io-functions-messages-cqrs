{
  "bindings": [
    {
      "authLevel": "function",
      "type": "cosmosDBTrigger",
      "name": "cosmosApiMessageStatusTrigger",
      "direction": "in",
      "connectionStringSetting": "COSMOSDB_CONNECTION_STRING",
      "databaseName": "%COSMOSDB_NAME%",
      "collectionName": "message-status",
      "leaseCollectionName": "cqrs-leases",
      "leaseCollectionPrefix": "CosmosApiMessageStatusChangeFeedForView",
      "createLeaseCollectionIfNotExists": true,
      "startFromBeginning": true
    },
    {
      "type": "eventHub",
      "name": "outputMessageStatus",
      "eventHubName": "%MESSAGE_STATUS_FOR_VIEW_TOPIC_NAME%",
      "connection": "MESSAGE_STATUS_FOR_VIEW_TOPIC_PRODUCER_CONNECTION_STRING",
      "direction": "out"
  }
  ],
  "scriptFile": "../dist/CosmosApiMessageStatusChangeFeedForView/index.js"
}
